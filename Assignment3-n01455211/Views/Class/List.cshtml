@model IEnumerable<Assignment3_n01455211.Models.Class>
@{
    ViewBag.Title = "Course List";
}



<div id="containerlist">
    <h2>Courses List</h2>
    <p>Following courses are included in the Web Development Program </p>
    @foreach (var Class in Model)
    {
        <div class="listitem row">
            <div class="col-md-12"><a href="/Class/Show/@Class.ClassId">@Class.Classcode - @Class.Classname</a></div>
        </div>
    }
</div>